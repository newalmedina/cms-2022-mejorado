{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-zip": "*",
        "astrotomic/laravel-translatable": "^11.9",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "clavel/basic": "@dev",
        "clavel/crud-generator": "@dev",
        "clavel/locations": "@dev",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/dbal": "^3.1",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "jenssegers/agent": "^2.6",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^8.65",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "laravelcollective/html": "^6.2",
        "nwidart/laravel-menus": "^7.0",
        "phpoffice/phpspreadsheet": "^1.18",
        "proengsoft/laravel-jsvalidation": "^4.6",
        "rappasoft/laravel-authentication-log": "^1.1",
        "santigarcor/laratrust": "^6.3",
        "yajra/laravel-datatables-oracle": "^9.18"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^3.2",
        "laravel/dusk": "^6.19",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.5.10",
        "squizlabs/php_codesniffer": "^3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": {
        "locations": {
            "type": "path",
            "url": "./packages/clavel/locations/"
        },
        "crud-generator": {
            "type": "path",
            "url": "./packages/clavel/crud-generator/"
        },
        "basic": {
            "type": "path",
            "url": "./packages/clavel/basic/"
        }
    }
}
